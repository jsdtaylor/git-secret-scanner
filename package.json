{
  "name": "git-secret-scanner",
  "version": "0.1.5",
  "author": "Justin Taylor @jsdtaylor",
  "bin": {
    "git-secret-scanner": "./bin/run"
  },
  "bugs": "https://github.com/jsdtaylor/git-secret-scanner/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@octokit/rest": "^18.0.7",
    "async-exec": "^1.1.0",
    "detect-secrets": "^1.0.6",
    "nodegit": "^0.27.0",
    "tslib": "^1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/nodegit": "^0.26.12",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/jsdtaylor/git-secret-scanner",
  "keywords": [
    "git",
    "secret",
    "scan",
    "repo",
    "security",
    "password",
    "check"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "git-secret-scanner",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "jsdtaylor/git-secret-scanner",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --reporter=lcov --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
